
@{
    ViewData["Title"] = "View";
}


<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <title>loginForma</title>
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/css/Login-Form-Dark.css" rel="stylesheet" />
    <link href="~/css/styles.css" rel="stylesheet" />
</head>
<body>
    <div class="login-dark">
        @using (Html.BeginForm("Login", "Home"))
        {
            <form method="post">
                <h2 class="sr-only">Login Form</h2>
                <div class="illustration"></div>
                <div class="form-group"><input class="form-control" type="text" name="etUsername" placeholder="Username"></div>
                <div class="form-group"><input class="form-control" type="password" name="etPassword" placeholder="Password"></div>
                <div class="btn-group" role="group">
                    <button class="btn btn-primary btn-lg" id="btnSignIn" type="submit" style="background: #40A15D; border-top-right-radius: 5px; border-bottom-right-radius: 5px;">Sign in</button>
                   </div>
                <div class="btn-group" role="group"></div>
               
               
            </form>
        }
    </div>
    <script src="~/js/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
</body>
</html>

